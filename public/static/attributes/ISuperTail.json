{
  "classId": "idm.componet.db.Isupertail",
  "comName": "下发跟踪",
  "className": "ISuperTail",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
          "type": "input",
          "layoutType": "inline",
          "text": "唯一标识",
          "bindKey": "ctrlId",
          "disabled": true,
          "default": "@[packageid]",
          "desc": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "input",
            "layoutType": "block",
            "text": "列表高",
            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
            "bindKey": "listHeight",
            "default": "calc(100vh - 200px)"
          }
        ]
      },
      {
        "type": "group",
        "text": "内层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "innerBox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "10px",
              "paddingRightVal": "",
              "paddingBottomVal": "10px",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "每项item",
            "bindKey": "innerItemBox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "10px",
              "paddingRightVal": "10px",
              "paddingBottomVal": "10px",
              "paddingLeftVal": "10px"
            }
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "innerItemColor",
            "placeholder": "请选择颜色",
            "default": {
                "hex": "#f5f5f5",
                "hex8": "#f5f5f5"
            }
          },
          {
            "type": "input",
            "layoutType": "block",
            "text": "间距item",
            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
            "bindKey": "innerItemLi",
            "default": "15px"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "名称",
            "desc": "名称颜色 例如：承办部门",
            "bindKey": "nameFont",
            "default": {
                "fontColors": {
                    "hex": "#333",
                    "hex8": "#333"
                }
            }
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "内容",
            "desc": "内容 例如：2024-02-01",
            "bindKey": "innerFont",
            "default": {
                "fontColors": {
                    "hex": "#2c2727",
                    "hex8": "#2c2727"
                }
            }
          }
        ]
      },
      {
        "type": "group",
        "text": "外层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
            {
              "type": "inlineGroup",
              "text": "宽高",
              "children": [
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "宽",
                  "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                  "bindKey": "width",
                  "width": "60px",
                  "default": "100%"
                },
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "高",
                  "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                  "bindKey": "height",
                  "width": "60px",
                  "default": "100%"
                }
              ]
            },
            {
              "type": "box",
              "layoutType": "block",
              "text": "内外边距",
              "bindKey": "ulbox",
              "default": {
                "marginTopVal": "",
                "marginRightVal": "",
                "marginBottomVal": "",
                "marginLeftVal": "",
                "paddingTopVal": "10px",
                "paddingRightVal": "",
                "paddingBottomVal": "10px",
                "paddingLeftVal": ""
              }
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "bgColor",
              "placeholder": "请选择颜色",
              "default": {
                  "hex": "#ffffff",
                  "hex8": "#ffffff"
              }
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "容器阴影",
              "bindKey": "boxShadow",
              "default": ""
            },
            {
              "type": "border",
              "layoutType": "block",
              "text": "边框",
              "bindKey": "boxborder"
            }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "接口参数",
            "bindKey": "dataSourceParamFunc",
            "desc": "接口参数：自定义函数参数：{_this: this}",
            "multiple": false
          },
          {
            "type": "dataSourceSelect",
            "layoutType": "inline",
            "text": "数据源",
            "desc": "用于选择数据源",
            "bindKey": "dataSourceForm"
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "一键催办",
            "bindKey": "hadnleCui",
            "desc": "接口参数：自定义函数参数：{_this: this, data: 列表数据}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "一键回收",
            "bindKey": "handleHui",
            "desc": "接口参数：自定义函数参数：{_this: this, data: 列表数据}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "每个催办",
            "bindKey": "hadnleFirstCui",
            "desc": "接口参数：自定义函数参数：{_this: this, item: item}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "每个回收",
            "bindKey": "handleFirstHui",
            "desc": "接口参数：自定义函数参数：{_this: this, item: item}",
            "multiple": false
          }
        ]
      },
      {
        "type": "group",
        "text": "字段设置",
        "desc": "设置组件中展示的对应字段",
        "children": [
          {
            "type": "dataSourceFiledSelect",
            "layoutType": "inline",
            "text": "承办部门",
            "bindKey": "underPart",
            "default": "underPart"
          },
          {
            "type": "dataSourceFiledSelect",
            "layoutType": "inline",
            "text": "签收人",
            "bindKey": "people",
            "default": "people"
          },
          {
            "type": "dataSourceFiledSelect",
            "layoutType": "inline",
            "text": "下发时间",
            "bindKey": "time",
            "default": "time"
          },
          {
            "type": "dataSourceFiledSelect",
            "layoutType": "inline",
            "text": "签收状态",
            "bindKey": "status",
            "default": "status"
          },
          {
            "type": "dataSourceFiledSelect",
            "layoutType": "inline",
            "text": "签收时间",
            "bindKey": "signTime",
            "default": "signTime"
          }
        ]
      }
  ]
}