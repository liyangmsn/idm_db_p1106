{
    "classId": "idm.componet.db.IFormSearch",
    "comName": "搜索表单",
    "className": "IFormSearch",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "group",
            "text": "基本设置",
            "children": [
                {
                    "type": "input",
                    "text": "内容索引",
                    "layoutType": "inline",
                    "bindKey": "fieldKey",
                    "default": "contentQuery"
                }
            ]
        },
        {
            "type": "group",
            "text": "外层样式",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "inlineGroup",
                    "text": "宽高",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "width",
                            "width": "60px",
                            "default": "100%"
                        },
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "height",
                            "width": "60px",
                            "default": "100%"
                        }
                    ]
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "ulbox"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "背景色",
                    "bindKey": "bgColor",
                    "placeholder": "请选择颜色",
                    "default": {
                        "hex": "#ffffff",
                        "hex8": "#ffffff"
                    }
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "容器阴影",
                    "bindKey": "boxShadow",
                    "default": ""
                },
                {
                    "type": "border",
                    "layoutType": "block",
                    "text": "边框",
                    "bindKey": "boxborder"
                }
            ]
        },
        {
            "type": "group",
            "text": "数据变动联动组件",
            "desc": "当数据有变动会联动这里选择的组件，把当前结果的对象传递给这些组件",
            "children": [
                {
                    "type": "pageModuleSelect",
                    "layoutType": "block",
                    "text": "需求组件范围",
                    "desc": "当前数据变动需要联动其他组件需求值（比如下拉框选项接口参数等）的组件范围",
                    "bindKey": "linkageDemandPageModule",
                    "multiple": true
                },
                {
                    "type": "pageModuleSelect",
                    "layoutType": "block",
                    "text": "结果组件范围",
                    "desc": "当前数据变动需要联动其他组件变更结果值的组件范围",
                    "bindKey": "linkageResultPageModule",
                    "multiple": true
                }
            ]
        }
    ]
}