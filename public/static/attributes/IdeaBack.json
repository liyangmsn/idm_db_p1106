{
  "classId": "idm.componet.db.Ideaback",
  "comName": "意见反馈",
  "className": "IdeaBack",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
          "type": "input",
          "layoutType": "inline",
          "text": "唯一标识",
          "bindKey": "ctrlId",
          "disabled": true,
          "default": "@[packageid]",
          "desc": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "input",
            "layoutType": "inline",
            "text": "弹框标题",
            "desc": "弹框标题",
            "bindKey": "dialogTitle",
            "default": "意见反馈"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "弹框标题",
            "desc": "弹框标题样式",
            "bindKey": "dialogTitleStyle"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "反馈描述",
            "desc": "反馈描述",
            "bindKey": "dialogDesc",
            "default": "反馈描述："
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "上传附件",
            "desc": "上传附件",
            "bindKey": "dialogUpload",
            "default": "上传附件："
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "选填",
            "bindKey": "dialogTian",
            "desc":"选填是否展示",
            "default": true
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "提交X轴",
            "bindKey": "dialogBtnX",
            "desc":"提交按钮定位：right",
            "default": "50%"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "提交Y轴",
            "bindKey": "dialogBtnY",
            "desc":"提交按钮定位：bottom",
            "default": "50px"
          }
        ]
      },
      {
        "type": "group",
        "text": "外层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
            {
                "type": "inlineGroup",
                "text": "宽高",
                "children": [
                    {
                        "type": "input",
                        "layoutType": "block",
                        "text": "宽",
                        "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                        "bindKey": "width",
                        "width": "60px",
                        "default": "100%"
                    },
                    {
                        "type": "input",
                        "layoutType": "block",
                        "text": "高",
                        "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                        "bindKey": "height",
                        "width": "60px",
                        "default": "100%"
                    }
                ]
            },
            {
                "type": "box",
                "layoutType": "block",
                "text": "内外边距",
                "bindKey": "ulbox"
            },
            {
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "背景色",
                "bindKey": "bgColor",
                "placeholder": "请选择颜色",
                "default": {
                    "hex": "#ffffff",
                    "hex8": "#ffffff"
                }
            },
            {
                "type": "input",
                "layoutType": "inline",
                "text": "容器阴影",
                "bindKey": "boxShadow",
                "default": ""
            },
            {
                "type": "border",
                "layoutType": "block",
                "text": "边框",
                "bindKey": "boxborder"
            }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "接口参数",
            "bindKey": "dataSourceParamFunc",
            "desc": "接口参数：自定义函数参数：{_this: this}",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "预览文件",
            "bindKey": "previewFileFunc",
            "desc": "接口参数：自定义函数参数：{_this: this, fileObj: fileObj}",
            "multiple": false
          }
        ]
      }
  ]
}