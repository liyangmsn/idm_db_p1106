        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "标题文字",
          "desc": "标题样式",
          "bindKey": "listTitleStyle",
          "default": {
              "fontColors": {
                  "hex": "#333",
                  "hex8": "#333"
              }
          }
        },
        {
          "type": "select",
          "layoutType": "inline",
          "text": "收缩图标",
          "bindKey": "showHideIcon",
          "desc":"展开收缩图标位置",
          "dictionary": [
              {
                "label": "不展示",
                "value": "none"
              },
              {
                  "label": "左侧",
                  "value": "left"
              },
              {
                  "label": "右侧",
                  "value": "right"
              }
          ],
          "default": "left"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "收缩宽度",
          "desc": "收缩图标在左侧的宽度: width",
          "bindKey": "showLeftWidth",
          "display": "@[showHideIcon=='left']",
          "default": "25px"
        },
        {
          "type": "select",
          "layoutType": "inline",
          "text": "按钮样式",
          "bindKey": "listRightStyle",
          "desc":"右侧按钮样式",
          "dictionary": [
              {
                "label": "气泡文字提示",
                "value": "tooltip"
              },
              {
                  "label": "文字在图标下方",
                  "value": "fontBottom"
              }
          ],
          "default": "fontBottom"
        }
      ]
    },
