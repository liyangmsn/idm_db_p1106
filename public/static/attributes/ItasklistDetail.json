{
  "classId": "idm.componet.db.itasklistDetail",
  "comName": "任务明细",
  "className": "ItasklistDetail",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
          "type": "input",
          "layoutType": "inline",
          "text": "唯一标识",
          "bindKey": "ctrlId",
          "disabled": true,
          "default": "@[packageid]",
          "desc": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "input",
            "layoutType": "inline",
            "text": "标题名称",
            "desc": "标题名称",
            "bindKey": "titleName",
            "default": "任务明细",
            "display": "@[titleShow=='show']"
          },
          {
            "type": "radio",
            "layoutType": "inline",
            "text": "权限显示",
            "desc": "设置组件是否根据权限显示, 开启时默认不显示, 当权限函数返回true才会显示",
            "bindKey": "powerJudge",
            "dictionary": [
                {
                    "label": "开启",
                    "value": "show"
                },
                {
                    "label": "关闭",
                    "value": "hidden"
                }
            ],
            "default": "hidden"
          },
          {
            "type": "radio",
            "layoutType": "inline",
            "text": "标题展示",
            "desc": "标题是否展示",
            "bindKey": "titleShow",
            "dictionary": [
                {
                    "label": "显示",
                    "value": "show"
                },
                {
                    "label": "隐藏",
                    "value": "hidden"
                }
            ],
            "default": "show"
          },
          {
            "type": "radio",
            "layoutType": "inline",
            "text": "手风琴",
            "desc": "手风琴开启则只会展示一个，其他都会关闭",
            "bindKey": "accordion",
            "dictionary": [
                {
                    "label": "开启",
                    "value": "show"
                },
                {
                    "label": "关闭",
                    "value": "hidden"
                }
            ],
            "default": "show"
          },
          {
            "type": "radio",
            "layoutType": "inline",
            "text": "展开按钮",
            "desc": "标题右侧收缩按钮是否展示",
            "bindKey": "showHideCloumn",
            "dictionary": [
                {
                    "label": "显示",
                    "value": "show"
                },
                {
                    "label": "隐藏",
                    "value": "hidden"
                }
            ],
            "default": "show"
          },
          {
            "type": "inputNumber",
            "layoutType": "inline",
            "text": "图片样式",
            "bindKey": "emptySize",
            "desc":"暂无数据图片高度：100",
            "default": 100
          }
        ]
      },
      {
        "type": "group",
        "text": "标题内容",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "radio",
            "layoutType": "inline",
            "text": "标题图标",
            "desc": "标题图标是否展示",
            "bindKey": "titleIconShow",
            "dictionary": [
                {
                    "label": "显示",
                    "value": "show"
                },
                {
                    "label": "隐藏",
                    "value": "hidden"
                }
            ],
            "default": "show"
          },
          {
            "type": "iconSelect",
            "layoutType": "inline",
            "text": "标题图标",
            "desc": "标题图标",
            "bindKey": "titleIcon",
            "default": ""
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "desc": "设置图标颜色，会覆盖主题色",
            "text": "图标颜色",
            "bindKey": "titleIconFontColor",
            "default": {}
          },
          {
            "type": "inputNumber",
            "layoutType": "inline",
            "text": "图标大小",
            "bindKey": "titleIconFontSize",
            "default": 18
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "标题文字",
            "desc": "标题样式",
            "bindKey": "titleStyle",
            "default": {
                "fontColors": {
                    "hex": "#333",
                    "hex8": "#333"
                }
            }
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "标题边距",
            "bindKey": "titleBox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "20px",
              "marginLeftVal": "",
              "paddingTopVal": "",
              "paddingRightVal": "",
              "paddingBottomVal": "3px",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "边框",
            "bindKey": "titleBorder",
            "default": {
              "border": {
                "top": {
                    "style": "solid",
                    "width": 0,
                    "widthUnit": "px",
                    "colors": {
                      "hex8": ""
                    }
                },
                "right": {
                    "style": "solid",
                    "width": 0,
                    "widthUnit": "px",
                    "colors": {
                      "hex8": ""
                    }
                },
                "bottom": {
                    "style": "solid",
                    "width": 1,
                    "widthUnit": "px",
                    "colors": {
                      "hex8": "#999"
                    }
                },
                "left": {
                    "style": "solid",
                    "width": 0,
                    "widthUnit": "px",
                    "colors": {
                      "hex8": ""
                    }
                }
            },
              "radius": {
                  "leftTop": {
                      "radius": 0,
                      "radiusUnit": "px"
                  },
                  "rightTop": {
                      "radius": 0,
                      "radiusUnit": "px"
                  },
                  "leftBottom": {
                      "radius": 0,
                      "radiusUnit": "px"
                  },
                  "rightBottom": {
                      "radius": 0,
                      "radiusUnit": "px"
                  }
                }
              }
          },
          {
            "type": "select",
            "layoutType": "inline",
            "text": "右侧模式",
            "bindKey": "titleRightModule",
            "desc":"可以选择展示容器或者svg选择展示",
            "dictionary": [
                {
                    "label": "容器",
                    "value": "module"
                },
                {
                    "label": "svg列表",
                    "value": "svgList"
                }
            ],
            "default": "svgList"
          },
          {
            "type": "table",
            "text": "右侧按钮组",
            "bindKey": "titleRightGroup",
            "tabText": "按钮[index]",
            "display": "@[titleRightModule=='svgList']",
            "children": [
              {
                "type": "input",
                "layoutType": "inline",
                "text": "tab标识",
                "bindKey": "key"
              },
              {
                "type": "iconSelect",
                "layoutType": "inline",
                "text": "按钮图标",
                "bindKey": "titleBtnIcon",
                "default": ""
              },
              {
                "type": "colorPicker",
                "layoutType": "inline",
                "desc": "设置图标颜色，会覆盖主题色",
                "text": "图标颜色",
                "bindKey": "titleIconColor",
                "default": {}
              },
              {
                "type": "inputNumber",
                "layoutType": "inline",
                "text": "图标大小",
                "bindKey": "titleSizeColor",
                "default": 18
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "右距离",
                "desc": "距离右侧距离",
                "bindKey": "titleRightDis",
                "default": "3px"
              },
              {
                "type": "actionSelect",
                "layoutType": "block",
                "text": "按钮点击自定义函数",
                "bindKey": "handleTitleFunc",
                "desc": "按钮点击自定义函数, 参数：{_this: this}",
                "multiple": false
              }
            ]
          }
        ]
      },
    {
