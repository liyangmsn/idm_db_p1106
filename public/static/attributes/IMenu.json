{
    "classId": "idm.componet.db.IMenu",
    "comName": "表格菜单",
    "className": "IMenu",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "select",
            "layoutType": "inline",
            "text": "数据来源",
            "bindKey": "sourceType",
            "dictionary": [
                {
                    "label": "数据源",
                    "value": "dataSource"
                },
                {
                    "label": "静态",
                    "value": "static"
                }
            ],
            "default": "static"
        },
        {
            "type": "dataSourceSelect",
            "layoutType": "inline",
            "text": "数据源",
            "bindKey": "dataSource",
            "default": "",
            "display": "@[sourceType=='dataSource']"
        },
        {
            "type": "table",
            "layoutType": "inline",
            "text": "静态标签",
            "tabText": "菜单[index]",
            "bindKey": "items",
            "display": "@[sourceType=='static']",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "菜单名称",
                    "bindKey": "label",
                    "default": ""
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "菜单标识",
                    "bindKey": "value",
                    "desc": "每个标签的标识，这个必须唯一且不能为空，否则会功能或数据异常，注意标识不能为：editable、key、idm_datakey",
                    "default": ""
                }
            ]
        },
        {
            "type": "group",
            "text": "数据变动联动组件",
            "desc": "当数据有变动会联动这里选择的组件，把当前结果的对象传递给这些组件",
            "children": [
                {
                    "type": "pageModuleSelect",
                    "layoutType": "block",
                    "text": "需求组件范围",
                    "desc": "当前数据变动需要联动其他组件需求值（比如下拉框选项接口参数等）的组件范围",
                    "bindKey": "linkageDemandPageModule",
                    "multiple": true
                },
                {
                    "type": "pageModuleSelect",
                    "layoutType": "block",
                    "text": "结果组件范围",
                    "desc": "当前数据变动需要联动其他组件变更结果值的组件范围",
                    "bindKey": "linkageResultPageModule",
                    "multiple": true
                }
            ]
        }
    ]
}