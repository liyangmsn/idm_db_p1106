{
    "classId": "idm.componet.db.IMenu",
    "comName": "菜单",
    "className": "IMenu",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": ""
        },
        {
            "type": "group",
            "text": "数据源",
            "desc": "",
            "children": [
                {
                    "type": "select",
                    "layoutType": "inline",
                    "text": "数据来源",
                    "bindKey": "sourceType",
                    "dictionary": [
                        {
                            "label": "接口",
                            "value": "api"
                        },
                        {
                            "label": "静态",
                            "value": "static"
                        }
                    ],
                    "default": "static"
                },
                {
                    "type": "dataSourceFiledSelect",
                    "layoutType": "inline",
                    "text": "接口",
                    "bindKey": "source",
                    "default": 100,
                    "display": "@[sourceType=='api']"
                },
                {
                    "type": "table",
                    "layoutType": "inline",
                    "text": "静态标签",
                    "tabText": "菜单[index]",
                    "bindKey": "items",
                    "display": "@[sourceType=='static']",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "标签",
                            "bindKey": "label",
                            "default": "标签"
                        },
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "标识",
                            "bindKey": "value",
                            "desc": "每个标签的标识，这个必须唯一且不能为空，否则会功能或数据异常，注意标识不能为：editable、key、idm_datakey",
                            "default": ""
                        }
                    ]
                }
            ]
        },
        {
            "type": "group",
            "text": "结果联动",
            "desc": "点击？查看结果联动设置用法指南",
            "children": [
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "联动响应",
                    "bindKey": "resType",
                    "desc": "选择接收其他组件传值后要执行的后续动作，也就是选择接收到的值的作用是什么，处理完后返回控件所需要的结果",
                    "dictionary": [
                        {
                            "label": "无",
                            "value": "none"
                        },
                        {
                            "label": "作为接口参数",
                            "value": "interfaceParam"
                        },
                        {
                            "label": "直接使用结果",
                            "value": "resultVal"
                        },
                        {
                            "label": "自定义函数",
                            "value": "customFun"
                        }
                    ],
                    "default": "none",
                    "styleType": "radio"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "参数名称",
                    "bindKey": "resInterfaceParam",
                    "default": "",
                    "display": "@[resType=='interfaceParam']"
                },
                {
                    "type": "textarea",
                    "layoutType": "inline",
                    "text": "接口地址",
                    "bindKey": "resInterfaceUrl",
                    "default": "",
                    "display": "@[resType=='interfaceParam']"
                },
                {
                    "type": "textarea",
                    "layoutType": "inline",
                    "text": "显示结果",
                    "bindKey": "resResultValField",
                    "desc": "根据接传值的数据格式指定结果集的字段，比如传的值为{data:张三}，则这里应该填写data，如果不填写则直接显示整个结果",
                    "default": "",
                    "display": "@[resType=='resultVal']"
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "自定义函数",
                    "bindKey": "resFunction",
                    "desc": "其他组件传值后会把值传递到此方法并且调用，函数返回数据格式为：字符或数字（单选模式）、数组形式（多选或标签模式）",
                    "display": "@[resType=='customFun']",
                    "multiple": false
                }
            ]
        }
    ]
}